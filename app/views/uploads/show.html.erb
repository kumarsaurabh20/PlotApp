<%- model_class = Upload -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:calib_file_name) %>:</strong></dt>
  <dd><%= @upload.calib_file_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:calib_content_type) %>:</strong></dt>
  <dd><%= @upload.calib_content_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:calib_file_size) %>:</strong></dt>
  <dd><%= @upload.calib_file_size %></dd>
  <dt><strong><%= model_class.human_attribute_name(:calib_updated_at) %>:</strong></dt>
  <dd><%= @upload.calib_updated_at %></dd>
  <dt><strong><%= model_class.human_attribute_name(:inten_file_name) %>:</strong></dt>
  <dd><%= @upload.inten_file_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:inten_content_type) %>:</strong></dt>
  <dd><%= @upload.inten_content_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:inten_file_size) %>:</strong></dt>
  <dd><%= @upload.inten_file_size %></dd>
  <dt><strong><%= model_class.human_attribute_name(:inten_updated_at) %>:</strong></dt>
  <dd><%= @upload.inten_updated_at %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              uploads_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_upload_path(@upload), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              upload_path(@upload),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
